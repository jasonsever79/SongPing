// Your SongPing database structure



generator client {

  provider = "prisma-client-js"

}



datasource db {

  provider = "sqlite"

  url      = "file:./songping.db"

}



model Pairing {
model Message {

  id        Int      @id @default(autoincrement())

  songTitle String

  artist    String

  url       String?

  createdAt DateTime @default(now())

}
  id        Int      @id @default(autoincrement())

  code      String   @unique

  createdAt DateTime @default(now())

}
